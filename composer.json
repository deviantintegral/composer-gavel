{
    "name": "deviantintegral/composer-gavel",
    "description": "Enforce a minimum Composer version for your team",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Berry",
            "email": "deviantintegral@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "composer-plugin-api": "^2.0"
    },
    "require-dev": {
        "composer/composer": "^2.8",
        "friendsofphp/php-cs-fixer": "3.93.0",
        "mikey179/vfsstream": "^1.6",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^11.0 || ^12.0"
    },
    "autoload": {
        "psr-4": {
            "Deviantintegral\\ComposerGavel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Deviantintegral\\ComposerGavel\\Tests\\": "tests"
        }
    },
    "extra": {
        "class": "Deviantintegral\\ComposerGavel\\ComposerVersionRequirement",
        "composer-version": "^2.8.12"
    }
}
